async () => {\n    try {\n      if (num % 2 !== 0) {\n        const res = await axios.patch(\n          `/user/update_role/${editUser._id}`,\n          {\n            role: checkAdmin ? 1 : 0,\n          },\n          {\n            headers: { Authorization: token },\n          }\n        );\n\n        setSuccess(res.data.msg);\n        setNum(0);\n      }\n    } catch (err) {\n      err.response.data.msg && setErr(err.response.data.msg);\n    }\n  };\n\n  useEffect(() => {\n    if (users.length !== 0) {\n      users.forEach((user) => {\n        if (user._id === id) {\n          setEditUser(user);\n          setCheckAdmin(user.role === 1 ? true : false);\n        }\n      });\n    } else {\n      history(\"/profile\");\n    }\n  }, [users, id, history]);\n\n  return (\n    <div>\n      <Helmet>\n        <title>EDIT USER</title>\n      </Helmet>\n      {err && showErrMsg(err)}\n      {success && showSuccessMsg(success)}\n      <div className=\"edit-page\">\n        <div className=\"row\">\n          <Button\n            className=\"btn-back\"\n            onClick={() => history.goBack()}\n            size={size}\n            type=\"primary\"\n            shape=\"round\"\n            icon={<ArrowLeftOutlined />}\n          >\n            Go Back\n          </Button>\n        </div>\n        <div className=\"col-left\">\n          <h2>Edit User</h2>\n          <div className=\"form-group\">\n            <label>Name </label>\n            <Input\n              type=\"text\"\n              name=\"name\"\n              value={editUser.name}\n              placeholder=\"Your name\"\n              disabled\n            />\n          </div>\n          <div className=\"form-group\">\n            <label>Email </label>\n            <Input\n              type=\"text\"\n              name=\"email\"\n              value={editUser.email}\n              placeholder=\"Your email address\"\n              disabled\n            />\n          </div>\n          <div>\n            <Checkbox